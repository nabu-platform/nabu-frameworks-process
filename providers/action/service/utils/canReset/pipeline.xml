    <structure collectionName="processActionInstances" definition="nabu.frameworks.process.crud.processActionInstance.types.output" enricher="nabu.frameworks.process.crud.processActionInstance.services.list" maxOccurs="unbounded" minOccurs="0" name="activatedIncomingInstances" persister="nabu.frameworks.process.crud.processActionInstance.batch.updateAll"/>
