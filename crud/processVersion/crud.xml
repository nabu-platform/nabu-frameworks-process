<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<crud>
    <allowHeaderAsQueryParameter>true</allowHeaderAsQueryParameter>
    <broadcastCreate>false</broadcastCreate>
    <broadcastUpdate>false</broadcastUpdate>
    <coreType>nabu.frameworks.process.database.types.processVersion</coreType>
    <createRole>$user</createRole>
    <deleteRole>$user</deleteRole>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>processDefinitionId</key>
        <operator>=</operator>
        <or>false</or>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>deprecated</key>
        <operator>is null</operator>
        <or>false</or>
        <alias>active</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>deprecated</key>
        <operator>is null</operator>
        <or>false</or>
        <alias>released</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <filters>
        <caseInsensitive>false</caseInsensitive>
        <key>released</key>
        <operator>is not null</operator>
        <or>false</or>
        <alias>released</alias>
        <input>true</input>
        <vary>false</vary>
    </filters>
    <foreignFields>
        <foreignName>processDefinitionId:name</foreignName>
        <localName>name</localName>
    </foreignFields>
    <foreignFields>
        <foreignName>processDefinitionId:comment</foreignName>
        <localName>comment</localName>
    </foreignFields>
    <getPermission>process.list</getPermission>
    <hooks>false</hooks>
    <listPermission>process.list</listPermission>
    <listRole>$user</listRole>
    <provider>nabu.services.crud.provider.basic.provider</provider>
    <restLimitToUser>true</restLimitToUser>
    <updateRole>$user</updateRole>
    <useExplicitLanguage>false</useExplicitLanguage>
    <useGlobalPermissionContext>false</useGlobalPermissionContext>
    <useLanguage>false</useLanguage>
    <useListOutputForCreate>false</useListOutputForCreate>
    <useListOutputForUpdate>false</useListOutputForUpdate>
    <useProjectAsPermissionContext>false</useProjectAsPermissionContext>
    <useServiceContextAsPermissionContext>false</useServiceContextAsPermissionContext>
    <useWebApplicationAsPermissionContext>false</useWebApplicationAsPermissionContext>
</crud>
